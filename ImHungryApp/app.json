{
  "expo": {
    "name": "ImHungri",
    "slug": "ImHungryApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "imhungri",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FFF5AB"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.imhungri",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photos so you can select a profile picture.",
        "NSCameraUsageDescription": "We need access to your camera so you can select a profile picture.",
        "NSLocationWhenInUseUsageDescription": "We use your location to find restaurants and food spots near you.",
        "NSUserNotificationsUsageDescription": "We send notifications to let you know when your friends are hungry or when there's a new match.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "imhungri",
            "CFBundleURLSchemes": [
              "imhungri"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFF5AB"
      },
      "edgeToEdgeEnabled": true,
      "package": "Adaluna.imhungry",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-image-picker",
      "expo-font",
      "expo-secure-store",
      "expo-asset"
    ],
    "extra": {
      "supabaseUrl": "${SUPABASE_URL}",
      "supabaseAnonKey": "${SUPABASE_ANON_KEY}",
      "googleClientId": "${GOOGLE_CLIENT_ID}",
      "eas": {
        "projectId": "7d0c2bef-b815-4809-8ea8-f3c1d72a8dde"
      }
    }
  }
}
