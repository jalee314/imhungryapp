{
  "expo": {
    "name": "ImHungri",
    "slug": "ImHungryApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "imhungri",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FFF5AB"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.imhungri",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "imhungri",
            "CFBundleURLSchemes": ["imhungri"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFF5AB"
      },
      "edgeToEdgeEnabled": true,
      "package": "Adaluna.imhungry"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store"
    ]
  }
}
